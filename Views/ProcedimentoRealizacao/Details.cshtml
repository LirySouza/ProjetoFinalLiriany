@model ProjetoFinalLiriany.Models.ProcedimentoRealizacao

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Procedimento Realizado</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente.ClienteId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Procedimento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Procedimento.ProcedimentoId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Colaborador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Colaborador.ColaboradorId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LocalRealizacao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LocalRealizacao.LocalRealizacaoId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataRealizacao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataRealizacao)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ObservacaoRealizacao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ObservacaoRealizacao)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.ProcedimentoRealizacaoId" class=" btn btn-primary">Editar</a> |
    <a asp-action="Index" class=" btn btn-primary">Voltar à Lista</a>
</div>
