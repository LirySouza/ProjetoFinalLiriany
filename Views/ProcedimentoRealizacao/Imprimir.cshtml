@model ProjetoFinalLiriany.Models.ProcedimentoRealizacao
@{
    Layout = null;
}
<link rel="stylesheet" href="~/css/Imprimir.css" />

<div>
    <main class="geral">
        <header class="cabecario">
            <div>
                <img src="~/img/NovaLogo.jpg" alt="" class="imagem">
            </div>
            <div class="titulo">
                <h1>Resultados Laboratoriais</h1>
            </div>
        </header>
        <hr class="linha3">
        <div class="caixa1">
            <span>PACIENTE:@Html.DisplayFor(model => model.Cliente.ClienteNome)</span>
            <span>SEXO: @Html.DisplayFor(model => model.Cliente.ClienteSexo)</span>
            <span>ENDEREÇO: @Html.DisplayFor(model => model.Cliente.ClienteEndereco)</span>
            <span>NÚMERO: @Html.DisplayFor(model => model.Cliente.ClienteNumero)</span>
            <span>Clínica: Oliveira´s Laboratory</span>
        </div>
        <br>
        <div class="caixa2">
            <span>DATA DA COLETA: @Html.DisplayFor(model => model.DataRealizacao)</span>
        </div>
        <div class="caixa6">
            <span>MÉTODO: PCR( Reação em Cadeia de Polimerase) - Em tempo real Método in house</span>
        </div>
        <hr class="linha1">
        <div class="caixa7">
            <span>RESULTADO:</span>
        </div>
        <div class="caixa10">
            <span>
                @Html.DisplayFor(model => model.ObservacaoRealizacao)
            </span>
        </div>
        <hr class="linha2">
        <div class="caixa8">
            <span>METODOLOGIA: Jafé Automatizado</span>
        </div>
        <div class="caixa9">
            <span>Laboratório de Análises Sanguíneas</span>
        </div>
    </main>

</div>
